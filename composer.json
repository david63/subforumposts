{
	"name": "david63/subforumposts",
	"type": "phpbb-extension",
	"description": "Lists the number of posts in a subforum.",
	"version": "2.1.0-RC1",
	"time": "2019-07-18",
	"keywords": [
		"phpbb",
		"extension",
		"subforum",
		"posts"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Extension Developer"
		}
    ],
	"require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[*] Sub forum posts",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.7"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/3.2",
        	"filename": "/subforumposts_version_file.json"
    	}
	}
}
