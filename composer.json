{
	"name": "david63/subforumposts",
	"type": "phpbb-extension",
	"description": "Lists the number of posts in a subforum.",
	"version": "3.1.0-RC1",
	"time": "2020-01-20",
	"keywords": [
		"phpbb",
		"extension",
		"subforum",
		"posts"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Extension Developer"
		}
    ],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[*] Sub forum posts",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "subforumposts_version_file.json",
			"ssl": false
    	}
	}
}
